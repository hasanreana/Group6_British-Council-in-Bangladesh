<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.group6_britishcouncilinbangladesh.Reana.FileReadFMController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TextArea fx:id="textArea" layoutX="24.0" layoutY="94.0" prefHeight="301.0" prefWidth="387.0">
            <font>
                <Font size="18.0" />
            </font>
        </TextArea>
        <Button layoutX="428.0" layoutY="94.0" mnemonicParsing="false" onAction="#readFromFile" prefHeight="122.0" prefWidth="168.0" text="Read from file" textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Button layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onAction="#goBack" prefHeight="54.0" prefWidth="79.0" text="←" textAlignment="CENTER" wrapText="true">
            <font>
                <Font name="Calibri" size="24.0" />
            </font>
        </Button>
        <Button layoutX="428.0" layoutY="342.0" mnemonicParsing="false" onAction="#logOut" prefHeight="54.0" prefWidth="168.0" text="Log Out" textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="24.0" />
            </font>
        </Button>
        <Label layoutX="168.0" layoutY="16.0" text="File Reading">
            <font>
                <Font size="48.0" />
            </font>
        </Label>
        <Label fx:id="label" alignment="CENTER" layoutX="428.0" layoutY="260.0" prefHeight="77.0" prefWidth="168.0" textAlignment="CENTER" wrapText="true">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
    </children>

</AnchorPane>
